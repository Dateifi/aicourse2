    "def intpow(x, n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return x * intpow(x, n-1)\n",
    "\n",
    "x = 5\n",
    "n= 4\n",
    "\n",
    "intpow(x, n)"
